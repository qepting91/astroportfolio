---
import Layout from "../layouts/Layout.astro";

const pageTitle = "Resume";

const resume = {
  personalInfo: {
    name: "Quinten Epting",
  },
  summary: `Equipped with over 7 years of investigative experience in various private sector industries, law enforcement, and military roles, I am deeply committed to safeguarding the vulnerable. My expertise, bolstered by a Master's in Criminal Justice/Forensic Psychology and proficiency in Python and data analysis.`,
  technicalProficiencies: {
    programming: ["Python", "JavaScript", "R (training)", "Go (training)"],
    databases: ["MongoDB (NoSQL)", "CockroachDB (SQL)", "Supabase (NoSQL)"],
  },
  experience: [
    {
      role: "Associate Intelligence Analyst",
      company: "ZeroFox",
      period: "March 2024 - Present",
      tasks: [
        "Utilize proprietary cybersecurity tools and databases to collect, monitor, and deliver physical security and information protection intelligence for federal clients.",
        "Create and deliver technical training programs to 15+ analysts.",
        "Created workflow automations using Python and Apps Script to assist in OSINT investigations",
      ],
    },
    {
      role: "Federal Staff Consultant",
      company: "A-lign",
      period: "May 2023 - March 2024",
      tasks: [
        "Served as a technical advisor for federal audits by translating complex security compliance guidelines into actionable client actions.",
        "Facilitated knowledge-sharing and specialized training on federal security standards.",
        "Responded to intricate compliance requests, offering thorough explanations and on-demand training while maintaining a customer-centric mindset.",
      ],
    },
    {
      role: "Developer",
      company: "Frontsight",
      period: "Jan 2023 - Present",
      tasks: [
          "Led a development team to create an automated web app for internal news article generation, saving 160 hours/month by automating tasks with Python.",
          "Developed two full stack web apps in Next.js, one of which is a custom OSINT Assistant chatbot, leveraging a vector database. The second is also a custom chatbot, leveraging a Graph database.",
          "Conducted OSINT research focused on geopolitical threats, financial crimes, and terrorism, leveraging advanced tools to gather and analyze data from open sources, deep web, and dark web.",
          "Developed versioning and documentation guidelines for internal projects, using Notion as a knowledge management system to streamline team workflows.",
          "Automations developed within Pipedream saving an average of 40hrs a week, integrating OpenAI prompts, tailored to mission objectives."
      ],
    },
    {
      role: "Risk Security Analyst",
      company: "PwC",
      period: "January 2020 - May 2021",
      tasks: [
        "Conducted thorough Anti-Money Laundering (AML) and Mortgage Fraud Investigations (MFI), applying expert knowledge of Federal Regulations such as the Bank Secrecy Act, USA PATRIOT Acts, Financial Crimes Enforcement Network (FinCEN), and the Office of Foreign Assets Control (OFAC).",
        "Specialized in detecting and preventing fraudulent mortgage activities, utilizing OSINT tools and techniques, and aligning with organizational cyber governance policies.",
        "Demonstrated expertise in software such as Alteryx, Tableau, Google Suite, UiPath, and Python scripting language to effectively analyze large amounts of data, identify patterns/trends, and deliver professional results on internal and external projects.",
      ],
    },
    {
      role: "Pharmaceuticals Investigator",
      company: "Nephron Pharmaceuticals",
      period: "March 2019 – October 2019",
      tasks: [
        "Conducted research and investigations into non-compliance areas, applying Root Cause Analysis and effective Corrective Action Plans to identify risks and ensure regulatory compliance, showcasing alignment with financial crimes investigation.",
        "Collaborated with cross-functional teams, driving policy and process improvements, and demonstrating expertise in risk mitigation and strategic thinking.",
      ],
    },
    {
      role: "Police Officer",
      company: "Batesburg-Leesville Police Department",
      period: "January 2018 – March 2019",
      tasks: [
        "Led the creation of the gang intelligence task force as Criminal Intelligence Analyst, conducting HUMINT, SOCMINT, and geolocation analyses on active gangs, closely aligned with trans-national organized crime investigation.",
        "Produced detailed incident reports and legal documents, showcasing excellent communication skills and ability to work closely with law enforcement and other agencies.",
      ],
    },
    {
      role: "Military Police",
      company: "United States Army",
      period: "March 2013 – June 2015",
      tasks: [
        "Secured crime scenes and processed evidence in a range of incidents, showcasing experience in law enforcement support, reconnaissance, and surveillance operations.",
        "Provided site security during emergency and internment operations, aligning with financial crime prevention, risk mitigation techniques, and multi-jurisdictional investigation responsibilities.",
      ],
    },
  ],
  education: [
    {
      degree: "M.S. Criminal Justice/Forensic Psychology",
      institution: "Liberty University",
      graduated: "2019",
    },
    {
      degree: "B.A. Criminology",
      institution: "University of South Carolina",
      graduated: "2012",
    },
    {
      degree: "Career Studies Certificate (C.S.C), Supervision/Business Administration",
      institution: "Germanna Community College",
    },
    {
      degree: "Full-Stack Bootcamp",
      institution: "SavvyCoders",
      skills: "HTML, CSS, Javascript, MongoDB, Agile",
      graduated: "2024",
    },
  ],
  certifications: [
    "Cloud+ Certification | Comptia (Active)",
    "Security+ Certification | Comptia (Expired April 2023)",
    "ITIL V4 Certification | Axelos (IT Service Management Framework)",
    "Jira Service Management: Administration",
    "ICAgile Certified Professional",
  ],
  personalProjects: [
    {
      name: "RiskRadar",
      technologies: "HTML, CSS, JavaScript, FullHunt API, MongoDB, Express.js",
      description: "Custom built API utilized to scrape the CISA alerts to a NoSQL database",
    },
    {
      name: "Domain Analysis",
      technologies: "Python, Google Colab",
      description: "Domain Analysis automation that combines multiple services into a PDF report",
    },
    {
      name: "Ransomware Tracker",
      technologies: "Streamlit, CockroachDB",
      description: "Developed a Python-based site analyzing data from CockroachDB, created visualization dashboards for ransomware attacks",
    },
  ],
};
---

<Layout pageTitle={pageTitle}>
  <h1>{resume.personalInfo.name}'s Resume</h1>
  <p>{resume.summary}</p>

  <h2>Technical Proficiencies</h2>
  <div class="proficiencies">
    <div class="proficiency-column">
      <h3>Programming</h3>
      <ul>
        {resume.technicalProficiencies.programming.map((skill) => <li>{skill}</li>)}
      </ul>
    </div>
    <div class="proficiency-column">
      <h3>Databases</h3>
      <ul>
        {resume.technicalProficiencies.databases.map((db) => <li>{db}</li>)}
      </ul>
    </div>
  </div>

  <h2>Professional Experience</h2>
  <ul>
    {resume.experience.map((item) => (
      <li>
        <h3>{item.role} at {item.company}</h3>
        <p>{item.period}</p>
        <ul>
          {item.tasks.map((task) => (
            <li>{task}</li>
          ))}
        </ul>
      </li>
    ))}
  </ul>

  <h2>Education</h2>
  <ul>
    {resume.education.map((edu) => (
      <li>
        <h3>{edu.degree}, {edu.institution}</h3>
        {edu.graduated && <p>Graduated: {edu.graduated}</p>}
        {edu.skills && <p>Skills: {edu.skills}</p>}
      </li>
    ))}
  </ul>

  <h2>Certifications</h2>
  <ul>
    {resume.certifications.map((certification) => <li>{certification}</li>)}
  </ul>

  <h2>Personal Projects</h2>
  <ul>
    {resume.personalProjects.map((project) => (
      <li>
        <h3>{project.name}</h3>
        <p>Technologies: {project.technologies}</p>
        <p>{project.description}</p>
      </li>
    ))}
  </ul>
</Layout>

<style>
  .container {
    max-width: 800px;
    margin: auto;
    padding: 20px;
    background-color: #fff;
    box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);
  }

  /* Typography */
  h1, h2, h3 {
    font-family: "Georgia", serif;
  }

  h1 {
    font-size: 20px;
    margin-bottom: 0.5em;
  }

  h2 {
    font-size: 18px;
    color: #2a5078;
    margin-top: 1em;
    margin-bottom: 0.5em;
  }

  h3 {
    font-size: 18px;
    margin-bottom: 0.2em;
  }

  p {
    margin-bottom: 12px;
  }

  /* Section & Layout */
  .section {
    margin-bottom: 2em;
  }

  .list {
    list-style-type: none;
    padding: 0;
  }

  .list-item {
    margin-bottom: 5px;
  }

  .list-item:before {
    content: "–";
    margin-right: 5px;
    color: #555;
  }

  /* Color & Contrast */
  .section-heading {
    color: #2a5078;
    border-bottom: 2px solid #2a5078;
    padding-bottom: 4px;
  }

  a {
    color: #2a5078;
    text-decoration: none;
  }

  a:hover {
    text-decoration: underline;
  }

  /* Skills & Keywords */
  .skill, .keyword {
    background-color: #e8f0fe;
    color: #333;
    padding: 2px 4px;
    border-radius: 4px;
    font-size: 14px;
    display: inline-block;
    margin-right: 5px;
    margin-bottom: 5px;
  }

  /* Responsive Design */
  @media (max-width: 768px) {
    .container {
      padding: 15px;
    }

    h1 {
      font-size: 24px;
    }

    h2 {
      font-size: 20px;
    }

    h3, .section-heading {
      font-size: 18px;
    }
  }

  /* Technical Proficiencies */
  .proficiencies {
    display: flex;
    justify-content: space-between;
  }
  .proficiency-column {
    width: 48%;
  }
</style>
